import java.util.*;
public class Main{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    sc.nextLine();
    String s=sc.nextLine();
    if(s.endsWith("tea")){
      System.out.println("Yes");
    }
    else{
      System.out.println("No");
    }
  }
}
